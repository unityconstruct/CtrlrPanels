                 luaMethodValid="1"/>
      <luaMethod luaMethodName="layerComboChanged" luaMethodCode="--&#13;-- Called when a modulator value changes&#13;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#13;-- @value    new numeric value of the modulator&#13;--&#13;layerComboChanged = function(mod, value)&#13;&#10;&#9;console (&quot;layerComboChanged&quot;)&#13;&#10;&#9;combo = panel:getCombo(&quot;layerVisibilityCombo&quot;)&#13;&#10;&#9;if combo ~= nil then&#13;&#10;&#9;&#9;id = combo:getSelectedId()&#13;&#10;&#9;&#9;&#13;&#10;&#9;&#9;if id == 1 then&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerRed&quot;):setVisible(true)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerGreen&quot;):setVisible(false)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerBlue&quot;):setVisible(false)&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;if id == 2 then&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerRed&quot;):setVisible(false)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerGreen&quot;):setVisible(true)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerBlue&quot;):setVisible(false)&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;if id == 3 then&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerRed&quot;):setVisible(false)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerGreen&quot;):setVisible(false)&#13;&#10;&#9;&#9;&#9;panel:getCanvas():getLayerByName(&quot;layerBlue&quot;):setVisible(true)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;end"
