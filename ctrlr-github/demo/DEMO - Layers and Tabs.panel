      <luaMethod luaMethodName="switchTabsOnSliderMove" luaMethodCode="&#13;&#10;--&#13;&#10;-- Called when a modulator value changes&#13;&#10;--&#13;&#10;&#13;&#10;switchTabsOnSliderMove = function(modulator, newValue)&#13;&#10;&#9;c = getComponent (&quot;component-tabs&quot;)&#13;&#10;&#9;if c ~= nil then&#13;&#10;&#9;&#9;c:setPropertyInt (&quot;uiTabsCurrentTab&quot;, newValue)&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;--&#13;&#10;-- this handy function will return our component&#13;&#10;-- and if it can't find it it will return NIL and&#13;&#10;-- give a nice warning window, we use that to avoid&#13;&#10;-- crashing the app by accessing some invalid pointers&#13;&#10;--&#13;&#10;&#13;&#10;function getComponent(componentName)&#13;&#10;&#9;modulator = panel:getModulatorByName(componentName)&#13;&#10;&#9;if modulator ~= nil then&#13;&#10;&#9;&#9;component = modulator:getComponent()&#13;&#10;&#9;&#9;if component ~= nil then&#13;&#10;&#9;&#9;&#9;return component&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;return nil&#13;&#10;&#9;&#9;end&#13;&#10;&#9;else&#13;&#10;&#9;&#9;return nil&#13;&#10;&#9;end&#13;&#10;end"
                 luaMethodLinkedProperty="" luaMethodSource="0" uuid="a64163c96eb054f75283f2ac4954c600"
                 luaMethodValid="1"/>
      <luaMethod luaMethodName="switchLayerVisibility" luaMethodCode="&#13;&#10;--&#13;&#10;-- Called when a modulator value changes&#13;&#10;--&#13;&#10;&#13;&#10;switchLayerVisibility = function(m, v)&#13;&#10;&#9;layer = panel:getCanvas():getLayerByName(m:getName())&#13;&#10;&#9;if v == 1 then&#13;&#10;&#9;&#9;layer:setVisible (true)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;layer:setVisible (false)&#13;&#10;&#9;end&#13;&#10;end"
                 luaMethodLinkedProperty="" luaMethodSource="0" uuid="a2d8df95ed8e54ec2cf87e2f473290b8"
                 luaMethodValid="1"/>
      <luaMethod luaMethodName="currentTabChanged" luaMethodCode="--&#13;&#10;-- Called when the current tab in an uiTabs component is changed&#13;&#10;--&#13;&#10;&#13;&#10;currentTabChanged = function(modulator,currentTabIndex)&#13;&#10;&#9;console (string.format (&quot;currentTabChanged tabIndex=%d&quot;, currentTabIndex))&#13;&#10;&#9;l = getComponent(&quot;label-current-tab&quot;)&#13;&#10;&#9;if l ~= nil then&#13;&#10;&#9;&#9;l:setPropertyString (&quot;uiLabelText&quot;, string.format (&quot;Current tab=%d&quot;, currentTabIndex))&#13;&#10;&#9;end&#13;&#10;end"
