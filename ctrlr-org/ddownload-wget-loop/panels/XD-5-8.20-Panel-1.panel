      <luaMethod luaMethodName="sysexLoad" luaMethodCode="local function BitAND(a,b)--Bitwise and&#13;&#10;    local p,c=1,0&#13;&#10;    while a&gt;0 and b&gt;0 do&#13;&#10;        local ra,rb=a%2,b%2&#13;&#10;        if ra+rb&gt;1 then c=c+p end&#13;&#10;        a,b,p=(a-ra)/2,(b-rb)/2,p*2&#13;&#10;    end&#13;&#10;    return c&#13;&#10;end&#13;&#10;--&#13;&#10;-- Called when a panel receives a midi message&#13;&#10;--&#13;&#10;-- For Kawai K4, written by Steve Hookings, October 2014&#13;&#10;-- NOTE: Originally I used bitshifting and bias to create same output&#13;&#10;-- as on K4 display. But since this was different to the ranges from Sysex&#13;&#10;-- and on my Ctrlr display, I decided to use straight output.&#13;&#10;-- There is a benefit in that the reverse sysex should be easier.&#13;&#10;-- And since one uses this function to receive the single sysex input&#13;&#10;-- from K4 (as response to IntNum change) there seems little need&#13;&#10;-- to make the output look like the synth -- pictorially you see the patch&#13;&#10;--&#13;&#10;sysexLoad = function(midiMessage)&#13;&#13;&#10;&#13;&#10;&#9;-- This variable stops index issues during panel bootup&#13;&#10;&#9;if panel:getRestoreState() == true or panel:getProgramState() == true then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;local size = midiMessage:getSize()&#13;&#10;&#9;if size &lt; 18 then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;console (&quot;Sysex Single-Internal Dump recognised&quot;)&#13;&#10;&#9;console (&quot;preparing to update panel...&quot;)&#13;&#10;&#9;local d = midiMessage:getData()&#13;&#10;&#9;local a = {}&#13;&#10;&#9;local aNum = {&quot;A&quot;, &quot;B&quot;, &quot;C&quot;, &quot;D&quot;}&#13;&#10;&#13;&#10;-- Is the data valid?&#13;&#10;&#9;if size == 140  -- F0 + 138  + F7&#13;&#10; and d:getByte(1) == 0x40 and ((d:getByte(2) &gt;= 0x00) and (d:getByte(2) &lt;= 0x0F)) &#13;&#10;                        and (d:getByte(3) == 0x20)&#13;&#10;                        and (d:getByte(4) == 0x00)&#13;&#10;                        and (d:getByte(5) == 0x04)&#13;&#10;                        -- 0 == int, 2 = ext - we only want internal&#13;&#10;                        and (d:getByte(6) == 0x00) -- 0 == int, 2 = ext&#13;&#10;                        -- programData.getByte(6) contains the patch number&#13;&#10;                        -- 0 ... 63 is single, 64 ... 127 is multi. We want single&#13;&#10;                        and ((d:getByte(7) &gt;= 0x00) and (d:getByte(7) &lt;= 0x3F)) then&#13;&#10;--&#13;&#10;--&#9;////////////////////////////// Assign Patch Name To Label /////////////////////////////&#13;&#10;&#13;&#10;&#9;&#9;console (&quot;assigning patch name...&quot;)&#13;&#10;&#9;&#9;local tA = panel:getModulatorByName (&quot;IntNum&quot;):getModulatorValue()&#13;&#10;&#9;&#9;local BNum = panel:getModulatorByName (&quot;IntNum&quot;):getModulatorValue() % 16 + 1&#13;&#10;&#9;&#9;local ANum&#13;&#10;&#9;&#9;console (string.format(&quot;%2d %2d&quot;, tA, BNum))&#13;&#10;&#9;&#9;if tA &gt;= 48 then&#13;&#10;&#9;&#9;&#9;ANum = &quot;ID&quot;&#13;&#10;&#9;&#9;elseif tA&gt;= 32 then&#13;&#10;&#9;&#9;&#9;ANum = &quot;IC&quot;&#13;&#10;&#9;&#9;elseif tA &gt;= 16 then&#13;&#10;&#9;&#9;&#9;ANum = &quot;IB&quot;&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;ANum = &quot;IA&quot;&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;local getPatchName = ANum .. &quot;-&quot; .. string.format(&quot;%2.2d&quot;, BNum) .. string.format(&quot; %.10s&quot;, d:getRange(8,17):toString())&#13;&#10;&#13;&#10;&#9;&#9;if getPatchName == &quot;                &quot; then &#13;&#10;&#9;&#9;&#9;panel:getComponent(&quot;PatchName&quot;):setProperty(&quot;uiLabelText&quot;, &quot;Untitled Patch&quot;, false)&#13;&#10;&#9;&#9;elseif getPatchName ~= nil then &#10;  &#9;&#9;&#9;panel:getComponent(&quot;PatchName&quot;):setProperty(&quot;uiLabelText&quot;, getPatchName, false)&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;console (getPatchName)&#13;&#10;&#13;&#10;--&#9;///////////////////////////// Assign Values To Modulators /////////////////////////////&#13;&#10;&#13;&#10;&#9;&#9;console (&quot;assigning modulator values...&quot;)&#13;&#10;&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;MainVolume&quot;):setModulatorValue(d:getByte(18), false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(19), 31)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;Effect&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(20), 7)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;EffCh&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(21), 3)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;SrcMode&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(21) / 4, 3)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;PolyMode&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(21) / 16, 1)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;AM_1_2&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(21) / 32, 1)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;AM_3_4&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(22) / 16, 3)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;VibShape&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(23), 15)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;BendRG&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;v = BitAND(d:getByte(23) / 16, 3)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;WhlAssign&quot;):setModulatorValue(v, false, false, false)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;VibSpeed&quot;):setModulatorValue(d:getByte(24), false, false, false)&#13;&#10;&#9;&#9;panel:getModulatorByName (&quot;ComWhlDep&quot;):setModulatorValue(d:getByte(25), false, false, false)&#13;&#10;&#13;&#10;&#9;&#9;console (&quot;AB up