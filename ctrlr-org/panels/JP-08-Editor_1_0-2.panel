        <luaMethod luaMethodName="upperlowerchanged" luaMethodCode="upperlowerchanged = function(mod, value, source)&#13;&#10;&#9;if panel:getBootstrapState() then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#9;&#13;&#10;&#9;if ((source == 2) or (source == 5)) then&#9;&#9;&#9;&#9;-- not send SysEx when parameter is changed by Synth &#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;DUAL, Upper/Lower changed&quot;)&#13;&#10;&#13;&#10;&#9;modulatorName = mod:getProperty('name')&#13;&#10;&#13;&#10;&#9;local param = 0&#13;&#10;&#9;if(ParamTable2['DUAL']['modulator'] == modulatorName) then&#13;&#10;&#9;&#9;param = panel:getModulatorByName(ParamTable2['DUAL']['modulator']):getModulatorValue()&#13;&#10;&#9;&#9;sendEachparam2(modulatorName, param)&#9;&#9;&#9;&#9;-- Send switch state to JP-08&#13;&#10;&#9;&#9;if(param == 0)then&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- Switch from ON to OFF&#13;&#10;&#9;&#9;&#9;ParamTable2['UPPPER/LOWER']['paramValue'] =0&#9;-- set Uppwer/Lower OFF&#13;&#10;&#9;&#9;&#9;setParamtoModulator2('UPPPER/LOWER')&#13;&#10;&#9;&#9;&#9;sendEachparam2(ParamTable2['UPPPER/LOWER']['modulator'], 0)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;elseif(ParamTable2['UPPPER/LOWER']['modulator'] == modulatorName) then&#13;&#10;&#9;&#9;if(panel:getModulatorByName(ParamTable2['DUAL']['modulator']):getModulatorValue() == 0) then&#9;-- DUAL mode OFF state&#13;&#10;&#9;&#9;&#9;ParamTable2['UPPPER/LOWER']['paramValue'] =0&#9;-- set Uppwer/Lower OFF&#13;&#10;&#9;&#9;&#9;setParamtoModulator2('UPPPER/LOWER')&#13;&#10;&#9;&#9;&#9;return&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;param = panel:getModulatorByName(ParamTable2['UPPPER/LOWER']['modulator']):getModulatorValue()&#13;&#10;&#9;&#9;sendEachparam2(modulatorName, param)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;-- Timer start to get patch&#13;&#10;&#9;timer:setCallback (1, getPatchtimerCallback)&#13;&#10;&#9;timer:startTimer(1, 100)&#13;&#10;end&#10;&#13;&#10;&#13;&#10;function getPatchtimerCallback(timerId)&#13;&#10;&#9;timer:stopTimer(timerId)&#13;&#10;&#13;&#10;&#9;-- Get patch parameter from JP-08&#13;&#10;&#9;paramDumpRequest()&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function sendEachparam2(modulatorName, param)&#9;&#9;-- Except patch name&#13;&#10;&#9;local sysExAddress = 0x00&#13;&#10;&#9;local sysExOffsetAddress = 0x00&#13;&#10; &#13;&#10;&#9;for key, value in pairs(ParamTable2) do&#13;&#10;&#9;&#9;if(modulatorName == ParamTable2[key]['modulator']) then&#13;&#10;&#9;&#9;&#9;sysExAddress = ParamTable2[key]['SysExAddress']&#13;&#10;&#9;&#9;&#9;sysExOffsetAddress = ParamTable2[key]['SysExOffsetAddress']&#13;&#10;&#9;&#9;&#9;break&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;Value Changed:&quot; .. modulatorName .. &quot;=&quot; .. param)&#13;&#10;--&#9;console(&quot;sysExAddress = &quot; .. sysExAddress)&#13;&#10;--&#9;console(&quot;sysExOffsetAddress = &quot; .. sysExOffsetAddress)&#13;&#10;&#13;&#10;&#9;local programData = MemoryBlock(&#13;&#10;&#9;&#9;&#9;&#9;&#9;{&#13;&#10;&#9;&#9;&#9;&#9;&#9;0xf0, &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x41, 0x10, 0x00, 0x00, 0x00, 0x1c, 0x12, 0x01, 0x10, &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x00,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- sysExAddress &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x00,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- sysExOffsetAddress&#13;&#10;&#9;&#9;&#9;&#9;&#9;0, &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- Payload Data&#13;&#10;&#9;&#9;&#9;&#9;&#9;0,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- CheckSum (#13)&#13;&#10;&#9;&#9;&#9;&#9;&#9;0xf7&#13;&#10;&#9;&#9;&#9;&#9;&#9;} )&#13;&#10;&#9;local PayLoadOffset = 12&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- Parameter start from this addresss&#13;&#10;&#9;local sum = 0&#13;&#10;&#13;&#10;&#9;-- Set Address&#13;&#10;&#9;programData:setByte(PayLoadOffset -2,sysExAddress)&#13;&#10;&#9;programData:setByte(PayLoadOffset -1,sysExOffsetAddress)&#13;&#10;&#9;sum = sum + sysExAddress + sysExOffsetAddress&#13;&#10;&#13;&#10;&#9;-- Set Value&#13;&#10;&#9;programData:setByte(PayLoadOffset,param)&#13;&#10;&#9;sum = sum + param&#13;&#10;&#13;&#10;&#9;-- CheckSum&#13;&#10;&#9;check_sum = (0x100 - ((0x01 + 0x10 + sum) % 256)) % 128&#13;&#10;&#9;programData:setByte(13,check_sum)&#13;&#10;&#13;&#10;&#9;-- Send Program Data by SysEx &#13;&#10;--&#9;console(modulatorName .. &quot; ProgramData = &quot; .. programData:toHexString(1))&#13;&#10;&#9;panel:sendMidiMessageNow(CtrlrMidiMessage(programData:toHexString(1)))&#13;&#10;&#9;sum = 0&#13;&#10;&#13;&#10;&#9;-- Timer start to prevent SysEx loop (not Used)&#13;&#10;--&#9;timer:setCallback (1, timerCallback)&#13;&#10;--&#9;timer:startTimer(1, 500)&#13;&#10;--&#9;SysExPreventTimer = 1&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;"
