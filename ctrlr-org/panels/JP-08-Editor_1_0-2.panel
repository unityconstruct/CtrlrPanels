        <luaMethod luaMethodName="loadPatchFile" luaMethodCode="loadPatchFile = function(mod, value)&#13;&#10;&#9;if panel:getBootstrapState() then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;load patch file&quot;)&#13;&#10;&#13;&#10;&#9;patchfile = utils.openFileWindow (&quot;Load Patch File&quot;, File(&quot;&quot;), &quot;*.PRM&quot;, true)&#13;&#10;&#10;&#9;if patchfile:existsAsFile() then&#13;&#10;--&#9;&#9;console(&quot;load patch file&quot;)&#13;&#10;&#13;&#10;&#9;&#9;-- Set all parameter to default value before read&#13;&#10;&#9;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;&#9;ParamTable[key]['paramValue'] = ParamTable[key]['defaultValue']&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;local programData&#13;&#10;&#9;&#9;programData = patchfile:loadFileAsString()&#13;&#10;--&#9;&#9;console(&quot;load Data:\n&quot; .. programData)&#13;&#10;&#13;&#10;&#9;&#9;local patchDataTable = {}&#13;&#10;&#13;&#10;&#9;&#9;-- Load each param and set value to ParamTable&#13;&#10;&#9;&#9;for loadPatchName, loadPatchValue in string.gmatch(programData, &quot;%c*([%w%s_]+%w+)%s*%(([%d%w%s]*)%);%c*&quot;) do&#13;&#10;--&#9;&#9;&#9;console(&quot;ParamName=&quot; .. loadPatchName )&#13;&#10;--&#9;&#9;&#9;console(&quot;NameLen=&quot; .. string.len(loadPatchName) )&#13;&#10;--&#9;&#9;&#9;console(&quot;ParamNum=&quot; .. loadPatchValue )&#13;&#10;--&#9;&#9;&#9;console(&quot;NumLen=&quot; .. string.len(loadPatchValue) )&#13;&#10;&#13;&#10;&#9;&#9;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;&#9;&#9;if((string.match(loadPatchName, ParamTable[key]['PatchFileParamName']) ~= nil)&#13;&#10;&#9;&#9;&#9;&#9;&#9;and (string.len(loadPatchName) == string.len(ParamTable[key]['PatchFileParamName']))) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;if(tonumber(loadPatchValue) ~= nil) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;ParamTable[key]['paramValue'] = tonumber(loadPatchValue)&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;ParamTable[key]['paramValue'] = loadPatchValue&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;--&#9;&#9;&#9;&#9;&#9;console(&quot;Load &quot; .. key .. &quot;=&quot; .. ParamTable[key]['paramValue'])&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;   &#9;&#9;end&#13;&#10;&#13;&#10;&#13;&#10;&#9;&#9;-- Set PATCH NUM not selected&#13;&#10;&#9;&#9;panel:getModulatorByName('PATCH_NUM'):setModulatorValue(-1, false, false, false)&#13;&#10;&#13;&#10;&#9;&#9;-- Send all parameter to UI Modulator&#13;&#10;&#9;&#9;setPatchtoModulator()&#13;&#10;&#13;&#10;&#9;&#9;-- Send Parameter by SysEx&#13;&#10;&#9;&#9;SendParam()&#13;&#10;&#10;&#9;end&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function setPatchtoModulator()&#13;&#10;&#9;-- Set all parameter to modulator value&#13;&#10;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;setParamtoModulator(key)&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function setParamtoModulator(key)&#13;&#10;&#9;&#13;&#10;&#9;if(ParamTable[key]['modulator'] == &quot;&quot;) then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;Set &quot; .. ParamTable[key]['modulator'] .. &quot;=&quot; .. ParamTable[key]['paramValue'])&#13;&#10;&#13;&#10;&#9;if(ParamTable[key]['modulatorType'] == &quot;Slider&quot;) then&#13;&#10;&#9;&#9;if(panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue() ~= ParamTable[key]['paramValue']) then&#13;&#10;&#9;&#9;&#9;panel:getModulatorByName(ParamTable[key]['modulator']):setModulatorValue(ParamTable[key]['paramValue'], false, false, false)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;Text&quot;) then&#9;&#9;-- PATCH NAME&#13;&#10;&#9;&#9;panel:getModulatorByName(ParamTable[key]['modulator']):getComponent():setPropertyString(&quot;uiLabelText&quot;,ParamTable[key]['paramValue'])&#13;&#10;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;ListBox&quot;) then&#13;&#10;&#9;&#9;local setvalue = 0&#13;&#10;&#9;&#9;setvalue = ParamTable[key]['modulatorMaxValue'] - ParamTable[key]['paramValue']&#13;&#10;&#9;&#9;if(panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue() ~= setvalue) then&#13;&#10;&#9;&#9;&#9;panel:getModulatorByName(ParamTable[key]['modulator']):setModulatorValue(setvalue, false, false, false)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;Other&quot;) then&#13;&#10;&#9;&#9;if(ParamTable[key]['modulator'] == &quot;ASSIGN MODE&quot;) then&#13;&#10;&#9;&#9;&#9;local param = 0&#13;&#10;&#9;&#9;&#9;local setvalue = 0&#13;&#10;&#9;&#9;&#9;param = ParamTable[key]['paramValue']&#13;&#10;&#9;&#9;&#9;if(param == 0) then&#9;&#9;-- POLY&#13;&#10;&#9;&#9;&#9;&#9;setvalue = 2&#13;&#10;&#9;&#9;&#9;elseif(param == 2) then&#9;-- SOLO&#13;&#10;&#9;&#9;&#9;&#9;setvalue = 0&#13;&#10;&#9;&#9;&#9;elseif(param == 3) then&#9;-- UNISON&#13;&#10;&#9;&#9;&#9;&#9;setvalue = 1&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;if(panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue() ~= setvalue) then&#13;&#10;&#9;&#9;&#9;&#9;panel:getModulatorByName(ParamTable[key]['modulator']):setModulatorValue(setvalue, false, false, false)&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function setParamtoModulator2(key)&#13;&#10;&#9;&#13;&#10;&#9;if(ParamTable2[key]['modulator'] == &quot;&quot;) then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;Set &quot; .. ParamTable2[key]['modulator'] .. &quot;=&quot; .. ParamTable2[key]['paramValue'])&#13;&#10;&#13;&#10;&#9;if(ParamTable2[key]['modulatorType'] == &quot;Button&quot;) then&#13;&#10;&#9;&#9;if(panel:getModulatorByName(ParamTable2[key]['modulator']):getModulatorValue() ~= ParamTable2[key]['paramValue']) then&#13;&#10;&#9;&#9;&#9;panel:getModulatorByName(ParamTable2[key]['modulator']):setModulatorValue(ParamTable2[key]['paramValue'], false, false, false)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;"
