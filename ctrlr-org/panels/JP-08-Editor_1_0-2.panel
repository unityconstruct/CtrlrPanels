        <luaMethod luaMethodName="receiveControlChange" luaMethodCode="function receiveControlChange(midiMessage)&#10;    local ControlChange = 0&#13;&#10;&#9;local controlNumber = 0&#13;&#10;&#9;local param = 0&#13;&#10;&#13;&#10;&#9;if (midiMessage:getType() == ControlChange) then&#13;&#10;--&#9;&#9;console(&quot;MIDI CC received&quot;)&#13;&#10;&#13;&#10;&#9;&#9;controlNumber = midiMessage:getLuaData():getByte(1)&#9;-- MIDI CC Control Number&#13;&#10;--&#9;&#9;console( &quot;Control Number = &quot; .. controlNumber)&#13;&#10;&#13;&#10;&#9;&#9;for key, value in pairs(ParamTable) do&#9;&#9;&#9;&#9;-- Search ParamTable&#13;&#10;&#9;&#9;&#9;if(ParamTable[key]['midiControlNumber'] ==  controlNumber) then&#13;&#10;&#9;&#9;&#9;&#9;if(ParamTable[key]['modulator'] ~= &quot;&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;requestEachparam(ParamTable[key]['modulator'])&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;return&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;for key, value in pairs(ParamTable2) do&#9;&#9;&#9;&#9;-- Search ParamTable2&#13;&#10;&#9;&#9;&#9;if(ParamTable2[key]['midiControlNumber'] ==  controlNumber) then&#13;&#10;&#9;&#9;&#9;&#9;if(ParamTable2[key]['modulator'] ~= &quot;&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;if((ParamTable2[key]['modulator'] == 'DUAL') or (ParamTable2[key]['modulator'] == 'UPPPER/LOWER')) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;paramDumpRequest()&#13;&#10;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;return&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;end&#13;&#10;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function requestEachparam(modulatorName)&#13;&#10;&#9;local sysExAddress = 0x00&#13;&#10;&#9;local sysExOffsetAddress = 0x00&#13;&#10;&#13;&#10;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;if(modulatorName == ParamTable[key]['modulator']) then&#13;&#10;&#9;&#9;&#9;sysExAddress = ParamTable[key]['SysExAddress']&#13;&#10;&#9;&#9;&#9;sysExOffs