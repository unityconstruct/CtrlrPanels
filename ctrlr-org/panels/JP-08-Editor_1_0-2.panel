        <luaMethod luaMethodName="modulatorValueChaned" luaMethodCode="SysExPreventTimer = 0&#13;&#10;&#13;&#10;modulatorValueChaned = function(mod, value, source)&#13;&#10;&#9;if panel:getBootstrapState() then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#9;&#13;&#10;&#9;if ((source == 2) or (source == 5)) then&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- not send SysEx when parameter is changed by Synth &#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;souce = &quot; .. source)&#13;&#10;&#9;modulatorName = mod:getProperty('name')&#13;&#10;&#13;&#10;&#9;local param = 0&#13;&#10;&#9;param = getParamfromModulatorValue(modulatorName)&#13;&#10;&#13;&#10;&#9;-- Send each parameter by SysEx&#13;&#10;&#9;sendEachparam(modulatorName, param)&#13;&#10;end&#10;&#13;&#10;&#13;&#10;function getParamfromModulatorValue(modulatorName)&#13;&#10;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;if(ParamTable[key]['modulator'] == modulatorName) then&#13;&#10;&#9;&#9;&#9;if(ParamTable[key]['modulatorType'] == &quot;Slider&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;return panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue()&#13;&#10;&#9;&#9;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;ListBox&quot;) then &#13;&#10;&#9;&#9;&#9;&#9;local value = 0&#13;&#10;&#9;&#9;&#9;&#9;value = panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue()&#13;&#10;&#9;&#9;&#9;&#9;return ParamTable[key]['modulatorMaxValue'] - value&#13;&#10;&#9;&#9;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;Text&quot;) then &#13;&#10;&#9;&#9;&#9;&#9;return ParamTable[key]['paramValue']&#9;&#9;-- not changed by UI Panel&#13;&#10;&#9;&#9;&#9;elseif(ParamTable[key]['modulatorType'] == &quot;Other&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;if(ParamTable[key]['modulator'] == &quot;ASSIGN MODE&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;local value = 0&#13;&#10;&#9;&#9;&#9;&#9;&#9;value = panel:getModulatorByName(ParamTable[key]['modulator']):getModulatorValue()&#13;&#10;&#9;&#9;&#9;&#9;&#9;if(value == 2) then&#9;&#9;-- POLY&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;return 0&#13;&#10;&#9;&#9;&#9;&#9;&#9;elseif(value == 0) then&#9;-- SOLO&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;return 2&#13;&#10;&#9;&#9;&#9;&#9;&#9;elseif(value == 1) then&#9;-- UNISON&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;return 3&#13;&#10;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;function sendEachparam(modulatorName, param)&#9;&#9;-- Except patch name&#13;&#10;&#9;local sysExAddress = 0x00&#13;&#10;&#9;local sysExOffsetAddress = 0x00&#13;&#10; &#13;&#10;&#9;for key, value in pairs(ParamTable) do&#13;&#10;&#9;&#9;if(modulatorName == ParamTable[key]['modulator']) then&#13;&#10;&#9;&#9;&#9;sysExAddress = ParamTable[key]['SysExAddress']&#13;&#10;&#9;&#9;&#9;sysExOffsetAddress = ParamTable[key]['SysExOffsetAddress']&#13;&#10;&#9;&#9;&#9;break&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;--&#9;console(&quot;Value Changed:&quot; .. modulatorName .. &quot;=&quot; .. param)&#13;&#10;--&#9;console(&quot;sysExAddress = &quot; .. sysExAddress)&#13;&#10;--&#9;console(&quot;sysExOffsetAddress = &quot; .. sysExOffsetAddress)&#13;&#10;&#13;&#10;&#9;local programData = MemoryBlock(&#13;&#10;&#9;&#9;&#9;&#9;&#9;{&#13;&#10;&#9;&#9;&#9;&#9;&#9;0xf0, &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x41, 0x10, 0x00, 0x00, 0x00, 0x1c, 0x12, 0x03, 0x00, &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x00,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- sysExAddress &#13;&#10;&#9;&#9;&#9;&#9;&#9;0x00,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- sysExOffsetAddress&#13;&#10;&#9;&#9;&#9;&#9;&#9;0, 0, &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- Payload Data&#13;&#10;&#9;&#9;&#9;&#9;&#9;0,&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- CheckSum (#14)&#13;&#10;&#9;&#9;&#9;&#9;&#9;0xf7&#13;&#10;&#9;&#9;&#9;&#9;&#9;} )&#13;&#10;&#9;local PayLoadOffset = 12&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- Parameter start from this addresss&#13;&#10;&#9;local sum = 0&#13;&#10;&#13;&#10;&#9;-- Set Address&#13;&#10;&#9;programData:setByte(PayLoadOffset -2,sysExAddress)&#13;&#10;&#9;programData:setByte(PayLoadOffset -1,sysExOffsetAddress)&#13;&#10;&#9;sum = sum + sysExAddress + sysExOffsetAddress&#13;&#10;&#13;&#10;&#9;-- Set Value&#13;&#10;&#9;local upper_byte = 0&#13;&#10;&#9;local lower_byte = 0&#13;&#10;&#9;upper_byte = bit.rshift(param, 4)&#13;&#10;&#9;lower_byte = param - upper_byte * 16&#13;&#10;&#9;programData:setByte(PayLoadOffset,upper_byte)&#13;&#10;&#9;programData:setByte(PayLoadOffset + 1,lower_byte)&#13;&#10;&#9;sum = sum + upper_byte + lower_byte&#13;&#10;&#13;&#10;&#9;-- CheckSum&#13;&#10;&#9;check_sum = (0x100 - ((0x03 + 0x00 + sum) % 256)) % 128&#13;&#10;&#9;programData:setByte(14,check_sum)&#13;&#10;&#13;&#10;&#9;-- Send Program Data by SysEx &#13;&#10;--&#9;console(modulatorName .. &quot; ProgramData = &quot; .. programData:toHexString(1))&#13;&#10;&#9;panel:sendMidiMessageNow(CtrlrMidiMessage(programData:toHexString(1)))&#13;&#10;&#9;sum = 0&#13;&#10;&#13;&#10;&#9;-- Timer start to prevent SysEx loop (not Used)&#13;&#10;--&#9;timer:setCallback (1, timerCallback)&#13;&#10;--&#9;timer:startTimer(1, 500)&#13;&#10;--&#9;SysExPreventTimer = 1&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;function timerCallback(timerId)&#13;&#10;&#9;timer:stopTimer(timerId)&#13;&#10;&#9;SysExPreventTimer = 0&#13;&#10;end&#13;&#10;&#13;&#10;"
